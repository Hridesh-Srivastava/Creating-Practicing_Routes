{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/random",
        "dest": "index.js"
      },
      {
        "src": "/jokes/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/filter",
        "dest": "index.js"
      },
      {
        "src": "/all",
        "dest": "index.js"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }
  
  